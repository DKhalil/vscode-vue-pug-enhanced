{
  "name": "vscode-vue-pug-enhanced",
  "displayName": "vscode-vue-pug-enhanced",
  "description": "Pug syntax for Vue files and enhanced Pug features",
  "publisher": "quanzaiyu",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.51.0"
  },
	"tags": [
    "vscode",
    "jade",
    "pug",
    "html2pug",
    "html2jade",
    "pug2html",
    "jade2html"
	],
	"categories": [
    "Snippets",
    "Programming Languages",
    "Formatters",
    "Other"
	],
  "activationEvents": [
    "onCommand:extension.transformHtmlToPug",
    "onCommand:extension.transformPugToHtml"
  ],
  "main": "./src/main.js",
  "contributes": {
  },
  "scripts": {
    "push": "git push origin master && git push coding master",
		"build": "vsce package -o vscode-vue-pug-enhanced.vsix",
		"install": "yarn build && code --install-extension vscode-vue-pug-enhanced.vsix",
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.51.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}
